{
  "*.{js,jsx,ts,tsx}": [
    "prettier --write",
    "eslint --fix --cache --cache-location ./node_modules/.cache/.eslintcache",
    "tsc --noEmit --incremental false"
  ],
  "*.{css,scss}": ["prettier --write"],
  "*.{json,md,yaml,yml}": ["prettier --write"],
  "*.html": ["prettier --write --parser html"]
}
